  active[0] = bus_space_read_4((sc)->memt, (sc)->memh, (0x98));
  active[1] = bus_space_read_4((sc)->memt, (sc)->memh, (0xd8));
  if (sc->sc_devid == ((0x5451 << 16) | 0x10b9)) {
   endch = 32;
  } else {
   endch = 64;
  }
  for (ch=0; ch<endch; ch++) {
   mask = 1 << (ch & 0x1f);
   if (active[(ch & 0x20) ? 1 : 0] & mask) {
    bus_space_write_4((sc)->memt, (sc)->memh, ((ch & 0x20) ? 0xd8 : 0x98), (mask));
    autri_reg_clear_4(sc,(ch & 0x20) ? 0xdc : 0xa4, mask);
    if (ch == sc->sc_play.ch_intr) {
     if (sc->sc_play.intr)
      sc->sc_play.intr(sc->sc_play.intr_arg);
    }
    if (ch == sc->sc_rec.ch_intr) {
     if (sc->sc_rec.intr)
      sc->sc_rec.intr(sc->sc_rec.intr_arg);
    }
    autri_reg_set_4(sc, (ch & 0x20) ? 0xdc : 0xa4, mask);
   }
  }
 }
 if (intsrc & 0x00000008) {
 }
 autri_reg_set_4(sc,0xb0,
  0x00008000 | 0x00000800 | 0x00000800);
 __mtx_leave(&audio_lock );
 return 1;
}
