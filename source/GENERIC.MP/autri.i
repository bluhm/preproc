int
autri_read_codec(void *sc_, u_int8_t index, u_int16_t *data)
{
 struct autri_codec_softc *codec = sc_;
 struct autri_softc *sc = codec->sc;
 u_int32_t status, addr, cmd, busy;
 u_int16_t count;
 switch (sc->sc_devid) {
 case ((0x2000 << 16) | 0x1023):
  addr = 0x44;
  cmd = 0x00008000;
  busy = 0x00008000;
  break;
 case ((0x2001 << 16) | 0x1023):
  addr = 0x48;
  cmd = 0x00000800;
  busy = 0x00000800 | 0x00000400;
  break;
 case ((0x7018 << 16) | 0x1039):
  addr = 0x44;
  cmd = 0x00008000;
  busy = 0x00008000 | 0x00004000;
  break;
 case ((0x5451 << 16) | 0x10b9):
  if (sc->sc_revision > 0x01)
   addr = 0x40;
  else
   addr = 0x44;
  cmd = 0x00008000;
  busy = 0x00008000;
  break;
 default:
  printf("%s: autri_read_codec : unknown device\n",
      sc->sc_dev.dv_xname);
  return -1;
 }
 for (count=0; count < 0xffff; count++) {
  if ((bus_space_read_4((sc)->memt, (sc)->memh, (addr)) & busy) == 0)
   break;
  delay(1);
 }
 if (count == 0xffff) {
  printf("%s: Codec timeout. Busy reading AC97 codec.\n",
      sc->sc_dev.dv_xname);
  return -1;
 }
 bus_space_write_4((sc)->memt, (sc)->memh, (addr), ((index & 0x7f) | cmd));
 for (count=0; count < 0xffff; count++) {
  status = bus_space_read_4((sc)->memt, (sc)->memh, (addr));
  if ((status & busy) == 0)
   break;
  delay(1);
 }
 if (count == 0xffff) {
