  bus_space_write_4((sc)->memt, (sc)->memh, (0xa0), (reg64 | channel));
  for (i=0; i<5; i++) {
   bus_space_write_4((sc)->memt, (sc)->memh, (0xe0 + i*sizeof(cr[0])), (cr[i]));
   ;
  }
  if (channel < 0x20) {
   bus_space_write_4((sc)->memt, (sc)->memh, (0xf4), (0x30000000));
   bus_space_write_4((sc)->memt, (sc)->memh, (0xf8), (0x30000000));
  }
 }
}
int
autri_trigger_output(void *addr, void *start, void *end, int blksize,
    void (*intr)(void *), void *arg, struct audio_params *param)
{
 struct autri_softc *sc = addr;
 struct autri_dma *p;
 ;
 sc->sc_play.intr = intr;
 sc->sc_play.intr_arg = arg;
 sc->sc_play.offset = 0;
 sc->sc_play.blksize = blksize;
 sc->sc_play.length = (char *)end - (char *)start;
 p = autri_find_dma(sc, start);
 if (!p) {
  printf("autri_trigger_output: bad addr %p\n", start);
  return (22);
 }
 sc->sc_play.dma = p;
 __mtx_enter(&audio_lock );
 autri_setup_channel(sc, 0x01, param);
 bus_space_write_4((sc)->memt, (sc)->memh, (0xa8), (0));
 autri_enable_interrupt(sc, sc->sc_play.ch_intr);
 autri_startch(sc, sc->sc_play.ch, sc->sc_play.ch_intr);
 __mtx_leave(&audio_lock );
 return 0;
}
int
autri_trigger_input(void *addr, void *start, void *end, int blksize, void (*intr)(void *), void *arg, struct audio_params *param)
{
 struct autri_softc *sc = addr;
 struct autri_dma *p;
 ;
 sc->sc_rec.intr = intr;
 sc->sc_rec.intr_arg = arg;
 sc->sc_rec.offset = 0;
 sc->sc_rec.blksize = blksize;
 sc->sc_rec.length = (char *)end - (char *)start;
 p = autri_find_dma(sc, start);
 if (!p) {
  printf("autri_trigger_input: bad addr %p\n", start);
  return (22);
 }
 sc->sc_rec.dma = p;
 __mtx_enter(&audio_lock );
 if (sc->sc_devid == ((0x2001 << 16) | 0x1023)) {
  autri_reg_set_4(sc, 0x40, 0x00000200);
  bus_space_write_1((sc)->memt, (sc)->memh, (0x73), (0x80 | sc->sc_rec.ch));
 }
 autri_setup_channel(sc, 0x02, param);
 autri_enable_interrupt(sc, sc->sc_rec.ch_intr);
 autri_startch(sc, sc->sc_rec.ch, sc->sc_rec.ch_intr);
 __mtx_leave(&audio_lock );
 return 0;
}
void
autri_enable_interrupt(struct autri_softc *sc, int ch)
{
 int reg64;
 reg64 = (ch & 0x20) ? 0xdc : 0xa4;
 ch &= 0x1f;
 autri_reg_set_4(sc, reg64, 1 << ch);
}
void
autri_disable_interrupt(struct autri_softc *sc, int ch)
{
 int reg64;
 reg64 = (ch & 0x20) ? 0xdc : 0xa4;
 ch &= 0x1f;
 autri_reg_clear_4(sc, reg64, 1 << ch);
}
void
autri_startch(struct autri_softc *sc, int ch, int ch_intr)
{
 int reg64;
 u_int32_t chmask;
 reg64 = (ch & 0x20) ? 0xb4 : 0x80;
 ch &= 0x1f;
 chmask = (1 << ch) | (1 << ch_intr);
 autri_reg_set_4(sc, reg64, chmask);
}
void
autri_stopch(struct autri_softc *sc, int ch, int ch_intr)
{
 int reg64;
 u_int32_t chmask;
 reg64 = (ch & 0x20) ? 0xb8 : 0x84;
 ch &= 0x1f;
 chmask = (1 << ch) | (1 << ch_intr);
 autri_reg_set_4(sc, reg64, chmask);
}
int
autri_midi_open(void *addr, int flags, void (*iintr)(void *, int),
    void (*ointr)(void *), void *arg)
{
 struct autri_softc *sc = addr;
 ;
 ;
 ;
 sc->sc_iintr = iintr;
 sc->sc_ointr = ointr;
 sc->sc_arg = arg;
 if (flags & 0x0001)
  autri_reg_clear_1(sc, 0x22, 0x08);
 if (flags & 0x0002)
  autri_reg_set_1(sc, 0x22, 0x10);
 return (0);
}
void
autri_midi_close(void *addr)
{
 struct autri_softc *sc = addr;
 ;
 tsleep(sc, 32, "autri", hz/10);
 sc->sc_iintr = ((void *)0);
 sc->sc_ointr = ((void *)0);
}
int
autri_midi_output(void *addr, int d)
{
 struct autri_softc *sc = addr;
 if ((bus_space_read_1((sc)->memt, (sc)->memh, (0x21)) & 0x40) != 0) {
  bus_space_write_1((sc)->memt, (sc)->memh, (0x20), (d));
  return 0;
 }
 return 1;
}
