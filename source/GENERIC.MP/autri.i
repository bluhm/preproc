 case ((0x2001 << 16) | 0x1023):
  addr = 0x44;
  cmd = 0x00000800;
  busy = 0x00000800;
  break;
 case ((0x7018 << 16) | 0x1039):
  addr = 0x40;
  cmd = 0x00008000;
