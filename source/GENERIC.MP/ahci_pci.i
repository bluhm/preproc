  magic = pci_conf_read(pa->pa_pc, pa->pa_tag,
      0x40);
  pci_conf_write(pa->pa_pc, pa->pa_tag,
      0x40,
      magic | 0x01);
  pci_conf_write(pa->pa_pc, pa->pa_tag, 0x08,
      0x01 << 24 |
      0x06 << 16 |
      0x01 << 8 |
      (((pa->pa_class) >> 0) & 0xff) << 0);
  pci_conf_write(pa->pa_pc, pa->pa_tag,
      0x40, magic);
 }
}
